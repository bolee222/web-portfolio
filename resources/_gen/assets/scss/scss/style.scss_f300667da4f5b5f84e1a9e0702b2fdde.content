/* Button style */
.btn {
  font-size: 15px;
  font-family: "Roboto", sans-serif;
  font-weight: 600;
  text-transform: capitalize;
  text-align: center;
  padding: 5px 20px;
  border-radius: 5px;
  border: 1px solid;
  position: relative;
  z-index: 1;
  transition: .2s ease; }
  .btn:hover, .btn:active, .btn:focus {
    outline: 0;
    box-shadow: none !important; }

.btn-style {
  position: relative; }

.btn-link {
  padding: 0;
  color: #1e7cfe;
  border: 0;
  text-decoration: underline; }
  .btn-link:hover {
    color: #1e7cfe; }

.btn-link-group a {
  padding: 0;
  color: #1e7cfe;
  border: 0;
  text-decoration: underline; }

.btn-primary {
  background: #2f2f2f;
  color: #fff;
  border-color: #1e7cfe; }
  .btn-primary:active, .btn-primary:hover, .btn-primary.focus, .btn-primary.active {
    background: #1e7cfe !important;
    border-color: #1e7cfe !important; }

.btn-outline-primary {
  background: transparent;
  color: #2f2f2f;
  border-color: #1e7cfe; }
  .btn-outline-primary:active, .btn-outline-primary:hover, .btn-outline-primary.focus, .btn-outline-primary.active {
    background: #1e7cfe !important;
    border-color: #1e7cfe !important;
    color: #fff; }

